{
  "name": "Seat-Reservation",
  "version": "1.0.0",
  "description": "Seat-Reservation with Google Apps Script",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fuyuki006/Seat-Reservation.git"
  },
  "scripts": {
    "login": "clasp login",
    "setup": "rimraf .clasp.json && mkdirp dist && clasp create --type sheets --title \"Seat-Reservation\" --rootDir ./dist && mv ./dist/.clasp.json ./.clasp.json",
    "deploy": "npx clasp push"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "mkdirp": "^1.0.4",
    "@google/clasp": "^2.4.1",
  }
}
