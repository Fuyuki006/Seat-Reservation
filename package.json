{
  "name": "Seat-Reservation",
  "version": "1.0.0",
  "description": "Seat-Reservation with Google Apps Script",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fuyuki006/Seat-Reservation.git"
  },
  "scripts": {
    "login": "clasp login",
    "setup": "rimraf .clasp.json && mkdirp dist && clasp create --type sheets --title \"Seat-Reservation\" --rootDir ./dist && mv ./dist/.clasp.json ./.clasp.json && rimraf dist",
    "deploy": "rimraf dist && npx clasp push",
    "build": "cross-env NODE_ENV=production webpack"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "mkdirp": "^1.0.4",
    "@google/clasp": "^2.4.1",
    "cross-env": "^7.0.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
